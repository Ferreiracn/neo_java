<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="{popupModel>/title} {i18n>title_COP}" contentWidth="30%" class="sapUiContentPadding">
		<content>
			<Title text="{i18n>totalQuantity_COP}: {popupModel>/totalQuantity}" textAlign="Left" width="100%" class="sapUiSmallMarginBottom"/>
			<Table id="idProductsTable">
				<columns>
					<Column width="60%">
						<Text text="{i18n>costObject_COP}" />
					</Column>
					<Column width="40%">
						<Text text="{i18n>plannedQuantity_COP}" />
					</Column>
				</columns>
			</Table>
		</content>
		<buttons>
			<Button text="{i18n>cancelButton}" press="handleClosePopUp" class="onLeftBT"/>
			<Button text="{i18n>save}" press="onPressPopupSave" class="onRightBT"/>
		</buttons>
		
	</Dialog>
</core:FragmentDefinition>